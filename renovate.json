{
  "packageRules": [
    {
      "matchPackageNames": ["xmldoc"],
      "prConcurrentLimit": 0,
      "prHourlyLimit": 0
    },
    {
      "matchPackageNames": ["chalk"],
      "prConcurrentLimit": 2,
      "prHourlyLimit": 2
    },
    {
      "matchPackageNames": ["chalk", "xmldoc"],
      "groupName": "multiple limits"
    },
    {
      "matchPackageNames": ["nodemon", "react"],
      "prPriority": 10
    },
    {
      "matchPackageNames": ["react-scripts"]
    },
    {
      "matchPackageNames": ["react-dom"],
      "groupName": "react-dom-group",
      "prConcurrentLimit": null,
      "prHourlyLimit": null
    },
    {
      "matchPackageNames": ["express"],
      "prConcurrentLimit": null,
      "prHourlyLimit": null
    }
  ]
}
